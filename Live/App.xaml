<Application x:Class="Live.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Live"
             StartupUri="Views/LiveWindow.xaml">
    <Application.Resources>

        <Color x:Key="Dark" A="255" R="78" G="78" B="78"/>
        <SolidColorBrush Color="{StaticResource Dark}" x:Key="DarkBrush"/>
    </Application.Resources>
</Application>
